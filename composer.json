{
  "name": "hershel-theodore-layton/sql-queryf-codegen",
  "bin": [
    "bin/sql-queryf",
    "bin/sql-queryf.hack"
  ],
  "description": "Customize your queryf DSL to your heart's content",
  "license": "MIT",
  "require": {
    "hhvm": ">=4.102",
    "hershel-theodore-layton/printf-state-machine": "<1",
    "hershel-theodore-layton/sql-queryf": "<1",
    "hershel-theodore-layton/static-type-assertion-code-generator": "<2"
  },
  "require-dev": {
    "hershel-theodore-layton/test-chain": "<1",
    "hershel-theodore-layton/expect": "<1"
  },
  "scripts": {
    "post-install-cmd": [
      ".github/workflows/prepare-environment.sh"
    ],
    "post-update-cmd": [
      ".github/workflows/prepare-environment.sh"
    ]
  },
  "config": {
    "allow-plugins": {
      "hhvm/hhvm-autoload": true
    }
  }
}